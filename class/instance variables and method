class Student{
    String name;
    int rollno;
    void display(){
        System.out.println("name: "+name);
        System.out.println("rollno: "+rollno);
    }
}
public class Main{
    public static void main(String[]args){
        Student s1=new Student();
        s1.name="victoriya";
        s1.rollno=240;
        s1.display();
    }
}
